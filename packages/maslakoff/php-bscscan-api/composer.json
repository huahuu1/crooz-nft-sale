{
  "name": "maslakoff/php-bscscan-api",
  "type": "library",
  "description": "PHP client for the bscscan API",
  "homepage": "https://github.com/maslakoff/php-bscscan-api",
  "keywords": ["bscscan", "ethereum", "api", "Bscscan.io"],
  "license": "MIT",
  "authors": [
    {
      "name": "Maslakou Ihar",
      "email": "igormaslakoff@gmail.com",
      "homepage": "https://github.com/maslakoff"
    }
  ],
  "support": {
    "email": "igormaslakoff@gmail.com"
  },
  "require": {
    "php": "^5.6 || ^7.0 || ^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5 || ^6.0",
    "mockery/mockery": "~0.8",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": { "Bscscan\\": "lib/Bscscan/" }
  },
  "autoload-dev": {
    "psr-4": { "Bscscan\\Tests\\": "tests/Bscscan/"}
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
